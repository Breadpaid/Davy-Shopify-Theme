{
  "name": "dawei-kickstart-template",
  "version": "1.0.0",
  "description": "a Modern Shopify kickstart template using Online Store 2.0",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "shopify theme dev",
    "build": "shopify theme build",
    "deploy": "shopify theme push",
    "watch:tailwind": "tailwindcss -i ./src/tailwind.css -o ./assets/tailwind.css --watch",
    "build:tailwind": "tailwindcss -i ./src/tailwind.css -o ./assets/tailwind.css --minify"
  },
  "dependencies": {
    "@shopify/theme-cart": "^4.1.0",
    "@shopify/theme-product": "^4.1.0",
    "@shopify/theme-sections": "^4.1.0"
  },
  "devDependencies": {
    "@shopify/cli": "^3.0.0",
    "@shopify/theme-lint": "^3.0.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.0.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.0",
    "sass": "^1.45.0",
    "tailwindcss": "^3.3.0"
  }
}
